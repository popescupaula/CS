      see_also    : "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/U_CAN_Ubuntu_16-04_LTS_V2R1_STIG.zip"
      cmd         : "/usr/bin/find / -type d -perm -0002 \! -user root -exec ls -lLd {} \\; | /usr/bin/awk '{print} END {if (NR == 0) print \"none\"}'"
