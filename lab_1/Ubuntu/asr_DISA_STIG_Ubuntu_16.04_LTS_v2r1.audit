          regex       : "^[\\s]*password_pbkdf2[\\s]"
          expect      : "^[\\s]*password_pbkdf2[\\s]+root[\\s]+grub.pbkdf2.[A-Za-z0-9\\.]+[\\s]*$"
        </custom_item>
